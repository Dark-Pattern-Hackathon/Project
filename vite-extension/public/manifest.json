{
    "manifest_version":3,
    "version":"1.0.0",
    "name":"Vite Extension",
    "action":{
        "default_popup":"index.html"
    },
    "permissions":[
        "scripting",
        "https://github.com"
    ],
    "host_permissions":[
        "http://*/*",
        "https://*/*"
    ],
    "icons":{
        "32":"icon_32.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'",
        "sandbox": "allow-scripts"
    }
}
